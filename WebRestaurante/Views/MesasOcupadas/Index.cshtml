        </td>
        <td style="color:#ffffff">
            @Html.DisplayFor(modelItem => item.CPersonas_Mesas)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Cod_MesasO }) |
            @Html.ActionLink("Details", "Details", new { id=item.Cod_MesasO }) |
